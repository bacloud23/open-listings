{
  "name": "open-listings",
  "version": "1.0.0",
  "description": "A performant listings website with maps and tags",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'libs/**/*.js'",
    "lint-fix": "eslint --fix 'libs/**/*.js'",
    "pretty": "prettier --ignore-unknown --write libs/**/*.js",
    "prepare": "husky install",
    "precommit": "npm run lint-fix && npm run pretty",
    "download-assets": "curl https://raw.githubusercontent.com/stamparm/ipsum/master/ipsum.txt > data/raw/ipsum.txt",
    "build": "sudo apt-get install -y build-essential golang-go && npm run download-assets && npm install && cd client && npm install && npm run build && curl -sf https://gobinaries.com/tj/node-prune | sh",
    "start": "node --trace-deprecation server",
    "start:dev": "cross-env DEBUG=app:* nodemon server",
    "stop": "pkill --signal SIGINT open-listings",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:e2e": "NODE_ENV=api tap && ava",
    "doc": "rimraf docs && jsdoc -c jsdoc.conf.json",
    "//postinstall": "node-prune"
  },
  "type": "module",
  "keywords": [
    "Node.js",
    "JavaScript",
    "MongoDB",
    "Maps",
    "web-app"
  ],
  "license": "",
  "engines": {
    "node": ">=18.17.0"
  },
